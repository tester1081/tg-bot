{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "dest": "/api"
    },
    {
      "src": "/fund-wallet",
      "dest": "/fund-wallet.html"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}
